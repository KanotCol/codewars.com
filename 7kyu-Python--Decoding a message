def decode(message):
    messageNew = ''
    dic = {'a':'z','b':'y','c':'x','d':'w','e':'v','f':'u','g':'t','h':'s','i':'r','j':'q','k':'p','l':'o','m':'n','n':'m','o':'l','p':'k','q':'j','r':'i','s':'h','t':'g','u':'f','v':'e','w':'d','x':'c','y':'b','z':'a'}
    for i in message:
        if i == ' ':
            messageNew += ' '
        else:
            messageNew += dic[i]
    return messageNew
